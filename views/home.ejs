<%- include("partials/header") -%>
<h1>Home</h1>
<p><%= paragraph_home %></p>
<%posts.forEach(function(post){%>
    <h1><%= post.title%></h1>
    <%if(post.body.length > 25){%>
    <%body = post.body.substring(0,24)+"...";%>
    <p><%= body%><a href="/posts/<%=post.title%>">Read More</a></p>
    <%}else{%>
    <p><%= post.body%></p>
    <%}%>
    <%})%>
<%- include("partials/footer") -%>